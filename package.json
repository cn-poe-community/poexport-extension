    "@types/pako": "^2.0.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "cn-poe-export-db": "^0.1.4",
    "cn-poe-translator": "^0.2.8",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "pob-building-creater": "^0.0.13",
    "prettier": "^3.0.3",
